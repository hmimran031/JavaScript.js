// Home Work Problem-1

var momGive = 1000;
var apple = 400;
var orange = 300;
var totalkhoroj = apple + orange;
var reminder = momGive - totalkhoroj;
console.log(reminder);

// Home Work Problem-2
var math = 75.25;
var bio = 65;
var cha = 80;
var phy = 35.45;
var bang = 99.5;
var sumMark = math + bio + cha + phy + bang;
var avarage = sumMark / 5;
avarage = avarage.toFixed(2);
console.log(avarage);

